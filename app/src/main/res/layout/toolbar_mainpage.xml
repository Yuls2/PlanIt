<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="65dp"
    android:background="@drawable/back_toolbar"
    app:contentInsetStart="0dp"
    android:id="@+id/toolbar">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/toolbar_img_planet"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/ic_icon_planit"
            android:layout_marginBottom="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>
        <TextView
            android:id="@+id/toolbar_img_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PLANiT"
            style="@style/tv_title_t1"
            android:textSize="8dp"
            android:layout_marginBottom="3dp"
            app:layout_constraintTop_toBottomOf="@+id/toolbar_img_planet"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>


        <ImageView
            android:id="@+id/toolbar_img_Notice"
            android:layout_width="29dp"
            android:layout_height="29dp"
            android:src="@drawable/ic_icon_notice"
            android:layout_marginRight="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.appcompat.widget.Toolbar>